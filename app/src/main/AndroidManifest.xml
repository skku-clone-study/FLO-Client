<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cloneflow">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_flo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_flo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".PlaylistActivity"/>
        <activity android:name=".infoactivities.InfoArtistActivity" />
        <activity android:name=".infoactivities.InfoSongActivity" />
        <activity android:name=".infoactivities.InfoAlbumActivity" />
        <activity android:name=".StreamingActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".useractivities.LoginActivity" />
        <activity android:name=".useractivities.LocalSigninInfoInputActivity" />
        <activity android:name=".useractivities.ClauseSigninActivity" />
        <activity android:name=".useractivities.LocalSigninActivity" />
        <activity android:name=".useractivities.PhoneAuthActivity" />
        <activity
            android:name=".useractivities.KakaoLoginPopupActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>